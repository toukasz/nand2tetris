/**
 * 4-way 16-bit multiplexor:
 * out = a if sel = 00
 *       b if sel = 01
 *       c if sel = 10
 *       d if sel = 11
 */
CHIP Mux4Way16 {
    IN a[16], b[16], c[16], d[16], sel[2];
    OUT out[16];
    
    PARTS:
    Not(in= sel[0], out= sel0Not);
    Not(in= sel[1], out= sel1Not);
    And(a= sel[0], b= sel[1], out= sel1);
    And(a= sel[0], b= sel1Not, out= sel2);
    And(a= sel0Not, b= sel[1], out= sel3);
    Mux16(a= a, b= d, sel= sel1, out= out1);
    Mux16(a= out1, b= b, sel= sel2, out= out2);
    Mux16(a= out2, b= c, sel= sel3, out= out);
}